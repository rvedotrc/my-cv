{
  "name": "my-cv",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "pnpx puppeteer browsers install firefox",
    "test:prettier": "prettier --check .",
    "format": "prettier --write .",
    "dev": "next dev --port 3001",
    "build": "next build",
    "pdf": "tsc --project tsconfig.pdf.json && rm -rf var/pdf && mkdir -p var/pdf && node dist/src/pdf/generate.js && ls -l var/pdf/*",
    "start": "next start",
    "test": "concurrently --group pnpm:test:* && pnpm test-node",
    "test-node": "echo 'WARNING: no node tests defined in this project'",
    "test:lint": "echo 'WARNING: no linting set up in this project'"
  },
  "dependencies": {
    "dotenv": "17.2.1",
    "next": "15.5.2",
    "puppeteer": "^24.18.0",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@types/node": "24.3.0",
    "@types/react": "19.1.12",
    "@types/react-dom": "19.1.9",
    "concurrently": "9.2.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "typescript": "5.5.4"
  }
}
